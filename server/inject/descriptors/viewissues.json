{
  "type": "object",
  "properties": {
    "isViewIssues": {
      "type": "identifier",
      "selector": "#buglist > tbody > tr:nth-child(1) > td > span:nth-child(1)",
      "control": "exists"
    },
    "issueslist": {
      "type": "array",
      "selectorAll": "#buglist tr[bgcolor]",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 3
          },
          "summary": {
            "type": "string",
            "selector": "a",
            "control": "action.link"
          }
        }
      }
    }
  },
  "connect": []
}
